{application, aptest,
 [
  {description, "Simple APNS Push Tester"},
  {vsn, "0.9.2"},
  {modules, [
        aptest_app,
        aptest_apnsv2,
        aptest_apnsv3,
        aptest_cmdline,
        aptest_main,
        aptest_util
    ]},
  {registered, [aptest_app]},
  {applications, [
                  kernel,
                  stdlib,
                  getopt,
                  uuid,
                  jsx,
                  apns_erl_util,
                  sc_util
                 ]},
  {included_applications, [chatterbox]},
  {mod, {aptest_app, []}},
  {env, []}
 ]}.

